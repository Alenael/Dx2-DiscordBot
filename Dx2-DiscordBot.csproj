    <RootNamespace>Dx2_DiscordBot</RootNamespace>
    <StartupObject>Dx2_DiscordBot.Program</StartupObject>
    <Copyright>u/AlenaelReal</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CsvHelper" Version="12.1.2" />
